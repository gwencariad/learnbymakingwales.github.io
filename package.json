    "license": "ISC",
    "devDependencies": {
      "browser-sync": "^2.26.7",
      "npm-run-all": "^4.1.5"
    },
    "dependencies": {
      "lbm-frontend": "https://gitpkg.now.sh/learnbymakingwales/lbm-frontend/package?main"
    },
    "browserslist": [
      "defaults"
    ]
  }
  